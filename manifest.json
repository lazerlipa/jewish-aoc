{
    "manifest_version": 2,
    "name": "Jewish AOC",
    "version": "1.2",
    "description": "Replace certain words in Advent of Code to make it Jewish-friendly",
    "content_scripts": [
        {
            "matches": ["*://adventofcode.com/*"],
            "exclude_globs": ["*/input"],
            "js": ["kosherify.js"],
            "run_at": "document_end"
        }
    ]
}